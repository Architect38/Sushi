    //         url: $(this).prop('action'),
    //         type: $(this).prop('method'),
    //         data: JSON.stringify(data),
    //         contentType: 'application/json',
    //         success: function (data) {
    //             AppService.refresh();
    //         },
    //         error: function (data) {
    //             console.log(data);
    //
